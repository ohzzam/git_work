<?xml version="1.0" encoding="UTF-8"?>

<driverGroup>
  <driver>
    <driverType>Oracle</driverType>
    <driverName>Oracle</driverName>
    <className>oracle.jdbc.driver.OracleDriver</className>
    <urlType>jdbc:oracle:thin:@%s:%d:%s</urlType>
    <driverFile>lib/jdbc/ojdbc8-21.1.0.0.jar;lib/jdbc/orai18n-21.1.0.0.jar</driverFile>
  </driver>
  <driver>
    <driverType>Altibase</driverType>
    <driverName>Altibase</driverName>
    <className>Altibase.jdbc.driver.AltibaseDriver</className>
    <urlType>jdbc:Altibase://%s:%d/%s</urlType>
    <driverFile>lib/jdbc/Altibase.jar</driverFile>
  </driver>
  <driver>
    <driverType>Goldilocks</driverType>
    <driverName>Goldilocks</driverName>
    <className>sunje.goldilocks.jdbc.GoldilocksDriver</className>
    <urlType>jdbc:goldilocks://%s:%d/%s</urlType>
    <driverFile>lib/jdbc/goldilocks6.jar</driverFile>
  </driver>
  <driver>
    <driverType>Tibero</driverType>
    <driverName>Tibero</driverName>
    <className>com.tmax.tibero.jdbc.TbDriver</className>
    <urlType>jdbc:tibero:thin:@%s:%d:%s</urlType>
    <driverFile>lib/jdbc/tibero6-jdbc.jar</driverFile>
  </driver>
  <driver>
    <driverType>SQLServer</driverType>
    <driverName>SQLServer</driverName>
    <className>com.microsoft.sqlserver.jdbc.SQLServerDriver</className>
    <urlType>jdbc:sqlserver://%s:%d;DatabaseName=%s;encrypt=true;trustServerCertificate=true</urlType>
    <driverFile>lib/jdbc/mssql-jdbc-11.2.0.jre8.jar</driverFile>
  </driver>
  <driver>
    <driverType>Cubrid</driverType>
    <driverName>Cubrid</driverName>
    <className>cubrid.jdbc.driver.CUBRIDDriver</className>
    <urlType>jdbc:cubrid:%s:%d:%s:::</urlType>
    <driverFile>lib/jdbc/cubrid_11.jar</driverFile>
  </driver>
  <driver>
    <driverType>PostgreSQL</driverType>
    <driverName>PostgreSQL</driverName>
    <className>org.postgresql.Driver</className>
    <urlType>jdbc:postgresql://%s:%d/%s</urlType>
    <driverFile>lib/jdbc/postgresql-42.2.23.jar</driverFile>
  </driver>
  <driver>
    <driverType>MariaDB</driverType>
    <driverName>MariaDB</driverName>
    <className>org.mariadb.jdbc.Driver</className>
    <urlType>jdbc:mariadb://%s:%d/%s</urlType>
    <driverFile>lib/jdbc/mariadb-java-client-2.7.3.jar</driverFile>
  </driver>
</driverGroup>
